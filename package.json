{
  "name": "mental-health-test",
  "version": "1.0.0",
  "description": "Приложение для тестирования психического здоровья с ИИ-анализом",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "init-db": "node server/scripts/init-db.js",
    "init-supabase": "node server/scripts/init-supabase.js",
    "migrate:add-email": "node server/scripts/migrate-add-email.js",
    "test-domain": "node test-domain.js",
    "build:test": "npm run build && NODE_ENV=production node server/index.js",
    "postbuild": "echo 'Build completed successfully'",
    "check-deployment": "node check-deployment.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.2",
    "socks-proxy-agent": "^8.0.5",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.64.0",
    "react-router-dom": "^6.18.0",
    "uuid": "^9.0.1",
    "vite": "^4.5.0",
    "@vitejs/plugin-react": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "antd": "^5.11.0",
    "@ant-design/icons": "^5.2.6",
    "@supabase/supabase-js": "^2.38.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
